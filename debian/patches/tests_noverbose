-	RRDTOOL_V="$RRDTOOL"
-	RRDTOOL=verbose_rrdtool
-fi
+#if [ -n "$VERBOSE" ] ; then
+#	RRDTOOL_V="$RRDTOOL"
+#	RRDTOOL=verbose_rrdtool
+#fi
 
 if [ -z "$RRDTOOL" ] ; then 
         RRDTOOL=$TOP_BUILDDIR/src/rrdtool
